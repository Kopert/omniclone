{
    "base": [
        "--checkers",
        "32",
        "--transfers",
        "8",
        "--create-empty-src-dirs",
        "--fix-case",
        "--ignore-case",
        "--stats-file-name-length",
        "0"
    ],
    "terminal": [
        "--verbose",
        "--progress",
        "--color",
        "ALWAYS"],
    "systemd": [
        "--stats",
        "0",
        "--syslog",
        "--log-level",
        "INFO"
    ],
    "bisync": [
        "--resilient",
        "--recover",
        "--max-lock",
        "5m",
        "--conflict-resolve",
        "newer",
        "--compare",
        "size,modtime,checksum",
        "--log-format",
        "date,time",
        "-M"
    ],
    "backup": ["--delete-excluded", "--copy-links"]
}
